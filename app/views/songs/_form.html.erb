<h1><%= @song.id ? 'Edit Song' : 'Create Song' %></h1>
<hr />
<%= form_for [@artist, @song] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title, required: true, autofocus: true %>
  <br />
  <%= f.label :genre %>
  <%= f.text_field :genre, required: true %>
  <br />
  <%= f.label :year %>
  <%= f.text_field :year, required: true %>
  <br />
  <%= f.label :collaboration %>
  <%= f.text_field :collaboration, required: true %>
  <%= f.submit %>
<% end %>


